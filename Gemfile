source "https://rubygems.org"

gem "jekyll", "~> 4.2"
gem "webrick"

# If you have any plugins, put them here!
group :jekyll_plugins do
  gem "jekyll-feed"
  gem 'jekyll-paginate-v2', github: 'mmistakes/jekyll-paginate-v2', :branch => 'jekyll-v4'
  gem "jekyll-seo-tag"
  gem "jekyll-fontawesome-svg"
  gem "bibtex-ruby", "~> 5"
  # for authoring
  gem "jekyll-compose"
  # to check everything's hooked up right
  gem "html-proofer"
  # revealify
  gem "jekyll-revealify-plugin", :git => "https://gitlab.cecs.anu.edu.au/lucy/jekyll-revealify-plugin.git", :branch => 'master'
end

group :development do
  gem "solargraph"
end
